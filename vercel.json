{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/mobile",
      "dest": "/mobile.html"
    },
    {
      "src": "/desktop",
      "dest": "/main.html"
    }
  ]
}
